@model FICTFeed.Framework.Shedule.Schedule
@for (int i = 0; i < Model.Weeks.Count; i++)
{
    <h4 title="Click to expand" class="spoiler-link">@Html.ResourceString("Week") @(i + 1)</h4>
    <div class="spoiler-body">
        @for (int q = 0; q < Model.Weeks[i].Days.Count; q++)
        {
            <h5 title="Click to expand" class="spoiler-link">@Html.ResourceString("Day") @(q + 1)</h5>
            <div class="spoiler-body">
                @for (int j = 0; j < Model.Weeks[i].Days[q].Lessons.Count; j++)
                {
                    <div title="Click to expand" class="spoiler-link">@Html.ResourceString("Lesson") @(j + 1)</div>
                    <div class="spoiler-body">
                        <div>@Html.EditorFor(m => m.Weeks[i].Days[q].Lessons[j].Name)</div>
                        <div>@Html.EditorFor(m => m.Weeks[i].Days[q].Lessons[j].Room)</div>
                        <div>@Html.EditorFor(m => m.Weeks[i].Days[q].Lessons[j].Teacher)</div>
                        <div>@Html.EditorFor(m => m.Weeks[i].Days[q].Lessons[j].Start)</div>
                        <div>@Html.EditorFor(m => m.Weeks[i].Days[q].Lessons[j].End)</div>
                        @Html.HiddenFor(m=>m.Weeks[i].Days[q].Lessons[j].Number)
                    </div>
                }
            </div>
        }
    </div>
}
